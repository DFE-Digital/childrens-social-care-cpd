@{
    Layout = "_ErrorLayout";
    ViewData["Title"] = "Page not found";
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-three-quarters">
        <h1 class="govuk-heading-xl">Page not found</h1>
        <p class="govuk-body">
            If you typed the web address, check it is correct.
        </p>
        <p class="govuk-body">
            If you pasted the web address, check you copied the entire address.
        </p>
        <p class="govuk-body">
            If the web address is correct or you selected a link or button, <a href="mailto:support-for-social-workers-team@education.gov.uk" class="govuk-link">contact the Support for social workers team</a> if you need to speak to someone.
        </p>
    </div>
</div>
